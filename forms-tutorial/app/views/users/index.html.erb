<h1>Users</h1>

<ul>
  <%@users.each do |user| %>
    <li>
      <a href=<%= users_path(user) %> >
        <%= user.username %>
      </a>
      <ul>
        <li>
          <%= user.email %>
        </li>
        <li>
          <%= user.password %>
        </li>
      </ul>
    </li>
  <% end %>
</ul>